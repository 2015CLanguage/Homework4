#include<stdio.h>
#include<math.h>
int main()
{
	int a,b,c;
	a=2,b=1,c=0;
	while(a<=1000)
	{
	    while(b<=a)
		    {
			      if((a%b)==0&&(a/b)!=1)
		            c=c+b;
		        if(b==a&&c==a) 
		         {
				        printf("%d its factors are ",a);
		            for(b=1;b<a;)
		            {
		            	if((a%b)==0)
		            	    {
		            	    	printf("%d ",b); 
							        }
		            	b++;   
					      }
					     printf("\n");
				     }    
		        b=b+1;    
	    	}
	    b=1,c=0;
	    a=a+1;
	}    
	return 0;
}
