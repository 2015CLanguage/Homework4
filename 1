# include <stdio.h>
int main()
{
	int n, s, i;
	for (n = 2; n<1000; n++)
	{
		s = 0;
		for (i = 1; i<n; i++){
			if ((n%i) == 0)
				s = s + i;
		}
		if (n == s)
		{
			printf("%d,its factors are ", n);
			for (i = 1; i<n; i++)
				if ((n%i) == 0)
					printf("%d ", i);
			printf("\n");
		}
	}
	return 0;
}
